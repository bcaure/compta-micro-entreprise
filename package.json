{
  "name": "compta",
  "version": "0.1.1",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.6.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "csv-parser": "^2.3.2",
    "csv-writer": "^1.5.0",
    "env-cmd": "^10.0.1",
    "fs": "0.0.1-security",
    "npm-watch": "^0.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "concurrently --kill-others \"craco start\" \"wait-on tcp:3000 && electron src/main.js\"",
    "start:web": "env-cmd -f .env.web craco start",
    "start:electron-react": "craco start",
    "start:electron-dev": "wait-on tcp:3000 && electron src/main.js",
    "build:web": "craco build",
    "build:electron": "env-cmd -f .env.electron craco build",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^7.1.11",
    "electron-builder": "^22.3.2",
    "wait-on": "^4.0.0"
  },
  "main": "electron.js"
}
